<% content_for :breadcrumb do %>
  <%= breadcrumbs ["Home",root_path], 'Dashboard' %>
<% end %>
<div class="mh4 mt4">
  <%cost_permission = RoleCheck.call(@current_user, Constants::APP_MOD_COST).result[:read] == true %>
  <%- if params[:facility_id] == "All" || params[:facility_id].blank? %>
    <%= react_component 'manager_dashboard/ManagerDashboardApp', {cost_permission: cost_permission, batches: @batches, facilityId: current_shared_facility_ids.map{|x| x.to_s}} %>
  <%-else%>
    <%= react_component 'manager_dashboard/ManagerDashboardApp', {cost_permission: cost_permission, batches: @batches, facilityId: current_facility&.id.to_s} %>
  <%end%>
</div>
