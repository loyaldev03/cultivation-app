<% content_for :breadcrumb do %>
  <%= breadcrumbs ["Home",root_path], 'Employee' %>
<% end %>
<div class="mh4 mt4">
  <%= react_component 'people_dashboard/PeopleDashboardApp',
    {
      defaultFacilityId: current_default_facility&.id.to_s,
      currentFacilityId: (params[:facility_id].blank? || params[:facility_id] == "All" ? current_user_facilities_ids.map{|x| x.to_s} : current_facility&.id.to_s),
      currentTime: Time.current
    }
  %>
</div>