<head>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <script src="https://cdn.polyfill.io/v2/polyfill.min.js"></script>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <% if defined? include_turbolink %>
    <script src="/assets/turbolinks/dist/turbolinks.js"></script>
  <% end %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  <% if defined? include_sidebar %>
    <%= javascript_include_tag 'sidebar', 'data-turbolinks-track': 'reload' %>
  <% end %>
  <%# <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %1> %>
  <title>Cannvas</title>
</head>
